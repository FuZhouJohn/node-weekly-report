{
  "name": "@haodademm/nwr",
  "version": "0.0.5",
  "packageManager": "pnpm@7.0.0",
  "description": "Node Weekly Report Tool",
  "license": "MIT",
  "author": "ZhuangJiNan <a492073467@gmail.com>",
  "repository": "git@github.com:FuZhouJohn/node-weekly-report.git",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "t": "bin/cli.mjs"
  },
  "bugs": {
    "url": "https://github.com/FuZhouJohn/node-weekly-report/issues"
  },
  "homepage": "https://github.com/FuZhouJohn/node-weekly-report#readme",
  "scripts": {
    "prepublishOnly": "npm run build",
    "test": "jest",
    "dev": "esno src/cli.ts",
    "build": "unbuild",
    "stub": "unbuild --stub",
    "release": "npx bumpp --commit --push --tag"
  },
  "dependencies": {
    "commander": "9.3.0",
    "dayjs": "1.11.2",
    "inquirer": "8.2.4"
  },
  "devDependencies": {
    "@types/inquirer": "8.2.1",
    "@types/jest": "27.5.1",
    "@types/node": "17.0.36",
    "esno": "0.16.3",
    "fast-glob": "3.2.11",
    "jest": "28.1.0",
    "ts-jest": "28.0.3",
    "ts-node": "10.8.0",
    "typescript": "4.7.2",
    "unbuild": "0.7.4"
  }
}
